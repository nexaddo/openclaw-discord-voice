{
  "name": "openclaw-discord-voice",
  "version": "0.1.0",
  "description": "Discord voice integration plugin for OpenClaw",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,md}\"",
    "build": "tsc",
    "test": "vitest run --reporter=verbose --no-coverage",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "openclaw",
    "discord",
    "voice",
    "ai",
    "speech-to-text",
    "text-to-speech"
  ],
  "author": "Spenser Austin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nexaddo/openclaw-discord-voice.git"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "eslint": "^8.57.1",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "ts-jest": "^29.4.6",
    "vitest": "^4.0.18"
  }
}
