{
  "name": "@openclaw/discord-plugin",
  "version": "0.1.0",
  "description": "Discord plugin with voice integration for OpenClaw",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "lint": "eslint src __tests__"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "@types/node": "^25.2.0",
    "vitest": "^4.0.18",
    "@types/ws": "^8.18.1",
    "discord.js": "^14.14.0"
  },
  "peerDependencies": {
    "openclaw": "^2026.2.2",
    "discord.js": "^14.14.0"
  },
  "dependencies": {
    "@openclaw/voice-extension": "workspace:*"
  },
  "openclaw": {
    "extensions": ["./index.ts"],
    "type": "discord-plugin"
  }
}
